        }
      },
      "additionalProperties": {
        "oneOf": [
          {
            "$ref": "#/$defs/model_configs"
          },
          {
            "$ref": "#/$defs/empty_directory"
          }
        ]
      }
    },
    "on_configuration_change": {
      "type": "string",
      "enum": [
        "apply",
        "continue",
        "fail"
      ]
    },
    "on_schema_change": {
      "type": "string",
      "enum": [
        "append_new_columns",
        "fail",
        "ignore",
        "sync_all_columns"
      ]
    },
