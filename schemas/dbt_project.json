{
    "title": "dbt_project",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "name",
        "version",
        "config-version"
    ],
    "properties": {
        "version": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "analysis-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "asset-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "clean-targets": {
            "$ref": "#/$defs/array_of_strings"
        },
        "config-version": {
            "type": "number",
            "default": 2
        },
        "dispatch": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "macro_namespace",
                    "search_order"
                ],
                "properties": {
                    "macro_namespace": {
                        "type": "string"
                    },
                    "search_order": {
                        "$ref": "#/$defs/array_of_strings"
                    }
                },
                "additionalProperties": false
            }
        },
        "docs-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "macro-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "model-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "models": {
            "$ref": "#/$defs/model_configs"
        },
        "on-run-end": {
            "$ref": "#/$defs/array_of_strings"
        },
        "on-run-start": {
            "$ref": "#/$defs/array_of_strings"
        },
        "profile": {
            "type": "string"
        },
        "query-comment": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "object",
                    "properties": {
                        "append": {
                            "$ref": "#/$defs/boolean_or_jinja_string"
                        },
                        "comment": {
                            "type": "string"
                        },
                        "job-label": {
                            "$ref": "#/$defs/boolean_or_jinja_string"
                        }
                    },
                    "additionalProperties": false
                }
            ]
        },
        "require-dbt-version": {
            "$ref": "#/$defs/string_or_array_of_strings"
        },
        "seed-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "seeds": {
            "$ref": "#/$defs/seed_configs"
        },
        "snapshot-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "snapshots": {
            "$ref": "#/$defs/snapshot_configs"
        },
        "sources": {
            "$ref": "#/$defs/source_configs"
        },
        "target-path": {
            "type": "string"
        },
        "test-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "tests": {
            "$ref": "#/$defs/test_configs"
        },
        "vars": {
            "type": "object"
        }
    },
    "additionalProperties": false,
    "$defs": {
        "array_of_strings": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "boolean_or_jinja_string": {
            "oneOf": [
                {
                    "type": "string",
                    "pattern": "\\{\\{.*\\}\\}"
                },
                {
                    "type": "boolean"
                }
            ]
        },
        "model_configs": {
            "title": "Model configs",
            "type": "object",
            "description": "Configurations set in the dbt_project.yml file will apply to all models that don't have a more specific configuration set.",
            "properties": {
                "+bind": {
                    "$ref": "#/$defs/boolean_or_jinja_string",
                    "default": false
                },
                "+database": {
                    "type": "string"
                },
                "+enabled": {
                    "$ref": "#/$defs/boolean_or_jinja_string"
                },
                "+incremental_strategy": {
                    "type": "string"
                },
                "+materialized": {
                    "type": "string"
                },
                "+persist_docs": {
                    "type": "object",
                    "properties": {
                        "columns": {
                            "$ref": "#/$defs/boolean_or_jinja_string",
                            "default": true
                        },
                        "relation": {
                            "$ref": "#/$defs/boolean_or_jinja_string",
                            "default": true
                        }
                    },
                    "additionalProperties": false
                },
                "+post-hook": {
                    "$ref": "#/$defs/array_of_strings"
                },
                "+pre-hook": {
                    "$ref": "#/$defs/array_of_strings"
                },
                "+schema": {
                    "type": "string"
                },
                "+sql_header": {
                    "type": "string"
                },
                "+tags": {
                    "$ref": "#/$defs/string_or_array_of_strings"
                }
            },
            "additionalProperties": {
                "$ref": "#/$defs/model_configs"
            }
        },
        "seed_configs": {
            "title": "Seed configs",
            "type": "object",
            "properties": {
                "+quote_columns": {
                    "$ref": "#/$defs/boolean_or_jinja_string"
                },
                "+schema": {
                    "type": "string"
                },
                "+tags": {
                    "$ref": "#/$defs/string_or_array_of_strings"
                }
            },
            "additionalProperties": {
                "$ref": "#/$defs/seed_configs"
            }
        },
        "snapshot_configs": {
            "title": "Snapshot configs",
            "type": "object",
            "properties": {
                "+target_schema": {
                    "type": "string"
                }
            },
            "additionalProperties": {
                "$ref": "#/$defs/snapshot_configs"
            }
        },
        "source_configs": {
            "title": "Source configs",
            "type": "object",
            "properties": {
                "+enabled": {
                    "$ref": "#/$defs/boolean_or_jinja_string"
                },
                "+tags": {
                    "$ref": "#/$defs/string_or_array_of_strings"
                }
            },
            "additionalProperties": {
                "$ref": "#/$defs/source_configs"
            }
        },
        "string_or_array_of_strings": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "$ref": "#/$defs/array_of_strings"
                }
            ]
        },
        "test_configs": {
            "title": "Test configs",
            "type": "object",
            "description": "Configurations set in the dbt_project.yml file will apply to all tests that don't have a more specific configuration set.",
            "properties": {
                "+alias": {
                    "type": "string",
                    "description": "Only relevant when `store_failures` is true"
                },
                "+database": {
                    "type": "string",
                    "description": "Only relevant when `store_failures` is true"
                },
                "+enabled": {
                    "$ref": "#/$defs/boolean_or_jinja_string"
                },
                "+error_if": {
                    "type": "string"
                },
                "+fail_calc": {
                    "type": "string"
                },
                "+limit": {
                    "type": "number"
                },
                "+schema": {
                    "type": "string",
                    "description": "Only relevant when `store_failures` is true"
                },
                "+severity": {
                    "type": "string",
                    "enum": [
                        "warn",
                        "error"
                    ]
                },
                "+store_failures": {
                    "$ref": "#/$defs/boolean_or_jinja_string"
                },
                "+tags": {
                    "$ref": "#/$defs/string_or_array_of_strings"
                },
                "+warn_if": {
                    "type": "string"
                }
            },
            "additionalProperties": {
                "$ref": "#/$defs/test_configs"
            }
        }
    }
}
